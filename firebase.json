{
  "hosting": {
    "site": "codoutor",
    "cleanUrls": true,
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [ {
      "source": "/",
      "destination": "/login",
      "type": 301
    } ]
  },
  "database": {
    "rules": "database.rules.json"
  }
}
